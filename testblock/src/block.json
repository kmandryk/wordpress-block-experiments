{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/testblock",
	"version": "0.1.0",
	"title": "Testblock",
	"category": "layout",
	"attributes": {
		"content": {
			"type": "string",
			"source": "html",
			"selector": "p"
